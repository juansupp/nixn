form( name='$ctrl.pru' ng-submit='$ctrl.submit($ctrl.pru)')
	nix-auto(
		required='true'
		name='marca'
		label='Marca'
		ng-model='$ctrl.model.marca'
		nx-data='$ctrl.nxData.marca'
		frm="$ctrl.pru.marca"
		ng-change='$ctrl.change()'
	)

	nix-auto(
		required='true'
		name='modelo'
		label='Modelo'
		ng-model='$ctrl.model.modelo'
		nx-data='$ctrl.nxData.modelo'
		frm="$ctrl.pru.modelo"
	)
	nix-cliente(
		ng-model='$ctrl.cliente'
		nx-frm='$ctrl.pru.cliente'
		full='true'
	)
	md-button( type='submit') Fin
